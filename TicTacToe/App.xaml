<Application x:Class="TicTacToe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TicTacToe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ImageSource x:Key="Cross">Resources/Cross_Icon.png</ImageSource>
        <ImageSource x:Key="Circle">Resources/Circle.png</ImageSource>
        <ImageSource x:Key="Restart">Resources/Power - Restart.png</ImageSource>
    </Application.Resources>
</Application>